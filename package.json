{
  "name": "graph-user-export",
  "version": "1.0.8",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesper1982atea/graph-user-export.git"
  },
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "package": "npm run build && rm -f graph-user-export-dist.zip && cd dist && zip -r ../graph-user-export-dist.zip . -x \"**/.DS_Store\"",
  "release:tag": "npm version patch -m 'chore(release): %s' && git push --follow-tags",
  "update:latest": "node scripts/update-from-release.js --dest dist --clean",
  "update:dry": "node scripts/update-from-release.js --dest dist --clean --dry-run"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
  "jszip": "^3.10.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^7.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
    "style-loader": "^4.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^4.0.0"
  }
}
